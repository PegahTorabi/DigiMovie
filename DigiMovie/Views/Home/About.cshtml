@{
    ViewBag.Title = "درباره ی ما";
}
<h2 class="w3-center">@ViewBag.Title</h2>

<div class="w3-container w3-center w3-border w3-border-orange w3-round w3-padding-32 w3-light-grey">
    <h4>انتقاد و پیشنهادات</h4>
    <input type="text" name="txt" placeholder="نام و نام خانوادگی" class="input-lg w3-border w3-left-align w3-round w3-animate-input" />
    <input type="text" name="txt" placeholder="ایمیل خود را وارد کنید..." class="input-lg w3-border w3-left-align w3-round w3-animate-input" />
    <br />
    <textarea style="width:500px;"></textarea>
    <div class="container w3-padding-left w3-margin-left w3-margin-right w3-padding-right">
        <div class="container w3-margin-left w3-margin-right">
            <button class="w3-margin-top w3-round btn-block w3-orange">ثبت</button>
        </div>
    </div>
</div>
<br style="" />
<div id="googleMap" style="width:100%;height:400px;"></div>

<script>
function myMap() {
var mapProp= {
    center: new google.maps.LatLng(35.722070, 51.458182),
    zoom:18,
};
var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
}
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDHk0HShpqGYmSagbpvfxYZWtNr4Phx1b8&callback=myMap"></script>
